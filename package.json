{
  "name": "hitobito",
  "license": "AGPL-3.0-only",
  "scripts": {
    "build": "esbuild app/javascript/*.* --bundle --sourcemap --format=esm --outdir=app/assets/builds --public-path=/assets",
    "prebuild:css": "bundle exec bin/rails css:generate_dynamic_files",
    "build:css": "sass ./app/assets/stylesheets/application.sass.scss:./app/assets/builds/application.css ./app/assets/stylesheets/print.sass.scss:./app/assets/builds/print.css ./app/assets/stylesheets/oauth.sass.scss:./app/assets/builds/oauth.css --no-source-map --load-path=node_modules"
  },
  "dependencies": {
    "@hotwired/stimulus": "^3.2.2",
    "@hotwired/turbo-rails": "^8.0.2",
    "@popperjs/core": "^2.11.6",
    "@rails/actiontext": "^6.0.3-3",
    "@tarekraafat/autocomplete.js": "^10.2.7",
    "bootstrap": "^5.2.0",
    "esbuild": "^0.21.5",
    "jquery": "~1.12.4",
    "jquery-ui": "~1.13.2",
    "jquery-ujs": "~1.2.2",
    "js-cookie": "~2.2.0",
    "moment": "~2.29.4",
    "sass": "^1.77.4",
    "stimulus": "^3.2.2",
    "tom-select": "^2.2.2",
    "trix": "^1.3.2"
  },
  "devDependencies": {},
  "resolutions": {
    "jquery": "~1.12.4"
  }
}
